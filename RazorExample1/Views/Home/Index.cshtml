@using RazorExample1.Models;
@model UrunKategoriModel
@{
    ViewBag.Title = "Index";
}

<h1>Kargolar</h1>
<hr />
<div class="col-md-12">
    <div class="alert alert-success" role="alert">
        <strong>@Model.SiparisSayisi</strong> adet ürün listelendi.
    </div>
</div>


@foreach (var siparis in Model.Siparisler)
{

    <div class="col-sm-6 col-md-4">
        <div class="thumbnail">
            <img src="data:image/jpeg;base64," alt="...">
            <div class="caption">
                <h2>@siparis.SiparisAdi</h2>
                <p>
                    @if (siparis.Aciklama.Length > 90)
                    {
                        @(siparis.Aciklama.Substring(0, 87) + "...")
                    }
                    else
                    {
                        @siparis.Aciklama
                    }


                </p>
                <p>
                    @*<a href="Home/Details/?id=@siparis.UrunId" class="btn btn-primary" role="button">Button</a>*@
                    
                    @Html.ActionLink("Ürünü İncele","Details",new {id=siparis.UrunId},new {@class="btn btn-primary" }) 

                </p>
            </div>
        </div>
    </div>

}







